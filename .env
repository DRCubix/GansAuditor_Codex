# Test environment configuration
ENABLE_GAN_AUDITING=true
ENABLE_SYNCHRONOUS_AUDIT=true
DISABLE_THOUGHT_LOGGING=false

# Codex CLI Configuration
CODEX_EXECUTABLE=codex
CODEX_TIMEOUT=30000
CODEX_REQUIRE_AVAILABLE=true
CODEX_VALIDATE_ON_STARTUP=false
CODEX_ALLOW_MOCK_FALLBACK=true

# Audit settings
AUDIT_TIMEOUT_SECONDS=10
MAX_CONCURRENT_AUDITS=3
MAX_CONCURRENT_SESSIONS=10

# Session management
SYNC_AUDIT_STATE_DIRECTORY=.test-mcp-gan-state
SESSION_CLEANUP_INTERVAL=300000
MAX_SESSION_AGE=3600000

# Completion criteria
SYNC_AUDIT_TIER1_SCORE=95
SYNC_AUDIT_TIER1_LOOPS=5
SYNC_AUDIT_TIER2_SCORE=90
SYNC_AUDIT_TIER2_LOOPS=8
SYNC_AUDIT_TIER3_SCORE=85
SYNC_AUDIT_TIER3_LOOPS=10
SYNC_AUDIT_HARD_STOP_LOOPS=15

# System prompt
GAN_AUDITOR_PROMPT_ENABLED=true
GAN_AUDITOR_PROMPT_VERSION=2.0.0